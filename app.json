{
    "expo": {
        "name": "Movie Info",
        "slug": "movieinfo",
        "version": "1.0.9",
        "orientation": "portrait",
        "icon": "./assets/images/icon.png",
        "scheme": "myapp",
        "userInterfaceStyle": "automatic",
        "splash": {
            "image": "./assets/images/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#000"
        },
        "assetBundlePatterns": [
            "**/*"
        ],
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/images/adaptive-icon.png",
                "backgroundColor": "#000"
            },
            "package": "com.sdjd.movieinfo",
            "versionCode": 9
        },
        "plugins": [
            "@react-native-firebase/app",
            "expo-router",
            [
                "expo-build-properties",
                {
                    "android": {
                        "compileSdkVersion": 34,
                        "targetSdkVersion": 34,
                        "buildToolsVersion": "34.0.0"
                    }
                }
            ],
            [
                "expo-image-picker",
                {
                    "photosPermission": "The app accesses your photos to set your profile."
                }
            ]
        ],
        "experiments": {
            "typedRoutes": true
        },
        "extra": {
            "router": {
                "origin": false
            },
            "eas": {
                "projectId": "7376db23-1d11-424c-93d6-a358d7032959"
            }
        }
    },
    "react-native-google-mobile-ads": {
        "android_app_id": "ca-app-pub-5125983390574582~9918349822"
    }
}